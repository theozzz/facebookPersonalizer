{
  "manifest_version": 2,
  "name": "Facebook Personalizer",
  "version": "0.1",
   "browser_action": {
    "default_icon": "images/drink_facebook.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
	"https://www.facebook.com/*"
      ],
      "js": ["jquery-2.1.4.min.js", "jscolor.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}